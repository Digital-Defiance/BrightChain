{
  "name": "brightchain",
  "version": "0.1.0",
  "license": "MIT",
  "workspaces": [
    "brightchain-lib",
    "brightchain-lib-e2e",
    "brightchain-api",
    "brightchain-api-e2e",
    "brightchain-api-lib",
